    <sbh:mutableProvenance>We get Pmyo2 from the C.elegans&apos; genomic DNA by PCR.</sbh:mutableProvenance>
    <sbh:topLevel rdf:resource="https://synbiohub.org/public/igem/BBa_K1634002/1"/>
