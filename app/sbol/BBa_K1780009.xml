    <dcterms:modified>2015-09-18T03:19:40Z</dcterms:modified>
    <sbh:mutableProvenance>The promoter comes from the DNA sequence of Lambda phage.</sbh:mutableProvenance>
    <sbh:topLevel rdf:resource="https://synbiohub.org/public/igem/BBa_K1780009/1"/>
